{
  "rust-analyzer.cargo-watch.enable": false,
  "rust-analyzer.serverPath": "rust-analyzer",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/opt/homebrew/bin/black",
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.infoSign": ">>",
  "diagnostic.hintSign": ">",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "css",
    "scss",
    "python"
  ],
  "explorer.width": 35,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]"
}