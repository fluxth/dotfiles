{
  "rust-analyzer.cargo-watch.enable": false,
  "rust-analyzer.serverPath": "rust-analyzer",
  "python.formatting.provider": "black",
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.infoSign": ">>",
  "diagnostic.hintSign": ">",
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
  ],
  "_coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "css",
    "scss",
    "svelte",
    "vue"
  ],
  "explorer.width": 35,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "svelte.enable-ts-plugin": true,
  "snippets.ultisnips.pythonPrompt": false
}